    "email": "chilijung@gmail.com",
    "url": ""
  },
  "keywords": [
    "slate",
    "helpers",
    "utils"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^5.1.6",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^1.20.2",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "prettier": "^1.9.2",
    "immutable": "^3.8.1",
    "lerna": "^2.9.0",
    "mocha": "^3.0.1",
    "precommit-hook-eslint": "^3.0.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^16.2.0",
    "read-metadata": "^1.0.0",
    "rimraf": "^2.5.4",
    "slate": "^0.33.0"
  },
  "scripts": {
    "build": "lerna exec --parallel -- babel src -d lib --copy-files",
    "build:watch": "lerna exec --parallel -- babel src -d lib --copy-files -w",
    "test": "lerna exec --parallel -- npm test",
    "lint:src": "prettier --write ./packages/*/src/**/*.js && eslint ./packages/*/src/**/*.js",
    "lint:test": "prettier --write ./packages/*/test/**/*.js && eslint ./packages/*/test/**/*.js"
  },
  "pre-commit": [
    "lint:src",
    "lint:test"
  ],
  "repository": "Canner/slate-plugins",
  "peerDependencies": {
    "immutable": "^4.0.0-rc.9",
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "slate": "^0.33.0"
  },
  "license": "MIT",
  "dependencies": {}
}
